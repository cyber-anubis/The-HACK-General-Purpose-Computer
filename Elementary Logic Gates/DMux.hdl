/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Not(in=sel,out=NotSel);
	And(a=in,b=NotSel,out=a);
	And(a=in,b=sel,out=b);
}
