/**
 * 16-bit bitwise Or:
 * for i = 0..15 out[i] = (a[i] or b[i])
 */

CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    Not16(in=a,out=NotA);
	Not16(in=b,out=NotB);
	And16(a=NotA,b=NotB,out=temp);
	Not16(in=temp,out=out);
}